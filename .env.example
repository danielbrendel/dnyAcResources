TWITTERBOT_ENABLE=false
TWITTERBOT_TAGS="#assaultcube #acgame #resources #gamedevelopment #gamedev #indiedev"
TWITTERBOT_APIKEY=
TWITTERBOT_APISECRET=
TWITTERBOT_ACCESS_TOKEN=
TWITTERBOT_ACCESS_TOKEN_SECRET=

BROADCAST_DRIVER=log
